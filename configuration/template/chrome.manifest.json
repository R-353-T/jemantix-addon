{
    "author": "R353T_",
    "content_scripts": [
        {
            "matches": ["https://*.certitudes.org/*"],
            "css": ["index.min.css"],
            "js": ["index.min.js"],
            "run_at": "document_start"
        }
    ],
    "description": "{{DESCRIPTION}}",
    "homepage_url": "https://jemantix.r353t.net",
    "icons": {
        "48": "asset/icon-48x48.png",
        "96": "asset/icon-96x96.png",
        "128": "asset/icon-128x128.png",
        "512": "asset/icon-512x512.png"
    },
    "manifest_version": 3,
    "name": "Jemantix",
    "short_name": "Jemantix",
    "version": "{{VERSION}}",
    "web_accessible_resources": [
        {
            "resources": [
                "asset/icon/png/close.png",
                "asset/icon/png/account.png",
                "asset/icon/png/cloud.png",
                "asset/icon/png/faq.png",
                "asset/icon/png/help.png",
                "asset/icon/png/history.png",
                "asset/icon/png/trophy.png",
                "asset/icon/png/info.png",

                "asset/font/ubuntu_mono/ubuntumono-bold-italic.ttf",
                "asset/font/ubuntu_mono/ubuntumono-bold.ttf",
                "asset/font/ubuntu_mono/ubuntumono-italic.ttf",
                "asset/font/ubuntu_mono/ubuntumono-regular.ttf"
            ],
            "matches": [ "https://*.certitudes.org/*" ]
        }
    ],
    "permissions": [
      "https://www.jemantix.r353t.net/",
      "https://jemantix.r353t.net/"
    ]
}